{
  "name": "freemarket",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "node dist/server.js",
    "build": "astro check && astro build && npm run copy-data", 
    "preview": "astro preview",
    "astro": "astro",
    "tsc": "tsc",
    "copy-data": "mkdir -p dist/api/data && cp src/api/data/*.json dist/api/data/", 
    "sync-data": "cp dist/api/data/*.json src/api/data/", 
    "watch-json": "nodemon --watch dist/api/data --exec 'npm run sync-data'" 
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.2",
    "astro": "^4.16.7",
    "express": "^4.21.1",
    "freemarket": "file:",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "typescript": "^5.6.3",
    "nodemon": "^2.0.20"
  }


  
}

