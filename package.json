{
  "name": "freemarket",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "node dist/server.js",
    "build": "astro check && astro build && npm run copy-data",
    "preview": "astro preview",
    "astro": "astro",
    "tsc": "tsc",
    "copy-data": "mkdir -p dist/api/data && cp src/api/data/*.json dist/api/data/",
    "sync-data": "cp dist/api/data/*.json src/api/data/",
    "watch-json": "nodemon --watch dist/api/data --exec 'npm run sync-data'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.2",
    "@types/jsonwebtoken": "^9.0.7",
    "astro": "^4.16.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "freemarket": "file:",
    "jsonwebtoken": "^9.0.2",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "nodemon": "^2.0.20",
    "typescript": "^5.6.3"
  }
}
